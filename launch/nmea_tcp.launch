<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="config_file" default="$(find nmea_ros_bridge)/config/udp_config.yaml" />

  <node name="nmea_tcp" pkg="nmea_ros_bridge" type="nmea_tcp" output="screen" respawn="true" respawn_delay="5">
    <rosparam command="load" file="$(arg config_file)"/>
  </node>
</launch>
